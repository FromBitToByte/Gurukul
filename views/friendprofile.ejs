<%- include('partials/side-nav'); -%>
<div class="tempH side-nav-content p-3">
  <div id="dd" class="tempH scheduled mb-3" style="justify-content: space-between">
    <p></p>
    <p></p>
  </div>
  <div class="tempH container">
    <div class="tempH card m-3">
      <div class="tempH card-title text-center theme-heading">Friend Profile</div>

      <div
        class="tempH card-image waves-effect waves-block waves-light"
        style="width: 20%; margin: auto"
      >
        <img
          class="tempH activator"
          src="https://i.pinimg.com/originals/51/f6/fb/51f6fb256629fc755b8870c801092942.png"
        />
      </div>
      <div class="tempH card-content">
        <span class="tempH card-title activator grey-text text-darken-4"
          ><%=friendDetails.name%><i class="tempH material-icons right"
            >more_vert</i
          ></span
        >
      </div>
      <div class="tempH card-reveal">
        <span class="tempH card-title grey-text text-darken-4"
          >Details<i class="tempH material-icons right">close</i></span
        >
        <p>Email: <%=friendDetails.email%></p>
        <p>Phone: 99XXXXXXX</p>
      </div>
    </div>
  </div>
  <canvas id="stat" class="mt-5 mb-5"></canvas>


</div>

<script>
  var teacherNav = document.querySelectorAll('#student-nav');
  teacherNav.forEach((teacherNav) => {
    teacherNav.innerHTML = '';
    const navLinks = `<a id="dshbrd" href="/student/dashboard" 
        ><i class="fas fa-desktop"></i><span>Dashboard</span></a
      >`;
    teacherNav.innerHTML = navLinks;
  });
  document.querySelectorAll('.hme').forEach(link => link.href = location.hostname);
</script>

<%- include('partials/footer'); -%>
